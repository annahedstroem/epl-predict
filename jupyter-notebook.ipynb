   "source": [
    "# I use sort_values and forward-fill to produce corresponding data points for the test set. \n",
    "test_epl.sort_values(['HomeTeam', 'AwayTeam','Date'], inplace=True)\n",
    "test_epl.fillna(method='ffill',inplace=True)"
   ]
